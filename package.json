{
  "name": "koporei",
  "version": "0.0.1",
  "description": "Sapper-like router for Koa, Express and more!",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/Olyno/koporei",
  "author": "Olyno <https://github.com/Olyno>",
  "license": "GPL-3.0",
  "private": false,
  "keywords": [
    "koporei",
    "router",
    "koa",
    "express"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn lint",
    "version": "yarn format",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "build": "tsc",
    "dev": "nodemon --config nodemon/dev.json",
    "test": "nodemon --config nodemon/test.json"
  },
  "devDependencies": {
    "@types/express": "4.17.3",
    "@types/koa": "2.11.2",
    "@types/node": "13.7.7",
    "express": "4.17.1",
    "koa": "2.11.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.4",
    "yarn": "1.22.0"
  }
}
