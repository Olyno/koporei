{
  "name": "koporei",
  "version": "0.0.1",
  "description": "Sapper-like router for Koa, Express and more!",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/Olyno/koporei",
  "author": "Olyno <https://github.com/Olyno>",
  "license": "GPL-3.0",
  "private": false,
  "keywords": [
    "koporei",
    "router",
    "koa",
    "express"
  ],
  "files": [
    "dist/src"
  ],
  "scripts": {
    "prepublishOnly": "npm run lint",
    "version": "npm run format",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "build": "tsc",
    "dev": "npm run build && npm run start",
    "start": "node dist/tests/koa"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/koa": "^2.0.52",
    "@types/node": "^12.12.7",
    "express": "^4.17.1",
    "koa": "^2.11.0",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
